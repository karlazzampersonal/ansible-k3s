---
apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: "{{  metadata_name_production }}"
  namespace: default
spec:
  secretName: "{{ production_secret_name }}"
  issuerRef:
    name: "{{ production_issuer_ref }}"
    kind: ClusterIssuer
  commonName: "{{ common_name }}"
  dnsNames:
    - "{{ dns_name_1 }}"
    - "{{ dns_name_2 }}"
